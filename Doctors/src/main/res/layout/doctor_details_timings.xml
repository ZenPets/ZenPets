                        android:textSize="15sp" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <android.support.v7.widget.AppCompatTextView
                                style="@style/CondensedBold"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="6dp"
                                android:text="MORNING"
                                android:textColor="@color/secondary_text"
                                android:textSize="14sp" />

                            <android.support.v7.widget.AppCompatTextView
                                style="@style/CondensedBold"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="6dp"
                                android:text="AFTERNOON"
                                android:textColor="@color/secondary_text"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <android.support.v7.widget.AppCompatTextView
                                android:id="@+id/txtWedMorning"
                                style="@style/LightNormal"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="6dp"
                                android:text="MORNING"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="13sp" />

                            <android.support.v7.widget.AppCompatTextView
                                android:id="@+id/txtWedAfternoon"
                                style="@style/LightNormal"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="6dp"
                                android:text="AFTERNOON"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="13sp" />
                        </LinearLayout>
                    </LinearLayout>

                    <android.support.v7.widget.AppCompatTextView
                        style="@style/NormalBold"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:padding="11dp"
                        android:text="THURSDAY"
                        android:textColor="@color/accent"
                        android:textSize="15sp" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <android.support.v7.widget.AppCompatTextView
                                style="@style/CondensedBold"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="6dp"
                                android:text="MORNING"
                                android:textColor="@color/secondary_text"
                                android:textSize="14sp" />

                            <android.support.v7.widget.AppCompatTextView
                                style="@style/CondensedBold"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="6dp"
                                android:text="AFTERNOON"
                                android:textColor="@color/secondary_text"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <android.support.v7.widget.AppCompatTextView
                                android:id="@+id/txtThuMorning"
                                style="@style/LightNormal"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="6dp"
                                android:text="MORNING"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="13sp" />

                            <android.support.v7.widget.AppCompatTextView
                                android:id="@+id/txtThuAfternoon"
                                style="@style/LightNormal"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="6dp"
                                android:text="AFTERNOON"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="13sp" />
                        </LinearLayout>
                    </LinearLayout>

                    <android.support.v7.widget.AppCompatTextView
                        style="@style/NormalBold"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:padding="11dp"
                        android:text="FRIDAY"
                        android:textColor="@color/accent"
                        android:textSize="15sp" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <android.support.v7.widget.AppCompatTextView
                                style="@style/CondensedBold"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="6dp"
                                android:text="MORNING"
                                android:textColor="@color/secondary_text"
                                android:textSize="14sp" />

                            <android.support.v7.widget.AppCompatTextView
                                style="@style/CondensedBold"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="6dp"
                                android:text="AFTERNOON"
                                android:textColor="@color/secondary_text"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <android.support.v7.widget.AppCompatTextView
                                android:id="@+id/txtFriMorning"
                                style="@style/LightNormal"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="6dp"
                                android:text="MORNING"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="13sp" />

                            <android.support.v7.widget.AppCompatTextView
                                android:id="@+id/txtFriAfternoon"
                                style="@style/LightNormal"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="6dp"
                                android:text="AFTERNOON"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="13sp" />
                        </LinearLayout>
                    </LinearLayout>

                    <android.support.v7.widget.AppCompatTextView
                        style="@style/NormalBold"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:padding="11dp"
                        android:text="SATURDAY"
                        android:textColor="@color/accent"
                        android:textSize="15sp" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <android.support.v7.widget.AppCompatTextView
                                style="@style/CondensedBold"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="6dp"
                                android:text="MORNING"
                                android:textColor="@color/secondary_text"
                                android:textSize="14sp" />

                            <android.support.v7.widget.AppCompatTextView
                                style="@style/CondensedBold"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="6dp"
                                android:text="AFTERNOON"
                                android:textColor="@color/secondary_text"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:orientation="horizontal">

                            <android.support.v7.widget.AppCompatTextView
                                android:id="@+id/txtSatMorning"
                                style="@style/LightNormal"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="6dp"
                                android:text="MORNING"
                                android:textColor="@android:color/primary_text_light"
                                android:textSize="13sp" />

